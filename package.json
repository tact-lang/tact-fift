{
  "name": "tact-fift",
  "version": "1.0.0",
  "main": "dist/index.js",
  "repository": "https://github.com/tact-lang/tact-fift.git",
  "author": "Steve Korshakov <steve@korshakov.com>",
  "license": "MIT",
  "scripts": {
    "gen:grammar": "ohm generateBundles --withTypes src/grammar/*.ohm",
    "gen:func": "ts-node ./scripts/compiler.ts",
    "gen": "yarn gen:grammar && yarn gen:func",
    "build": "tsc --declaration",
    "test": "jest",
    "clean": "rm -fr dist",
    "release": "yarn clean && yarn build && yarn test && release-it --npm.yarn1"
  },
  "devDependencies": {
    "@ohm-js/cli": "^2.0.0",
    "@release-it/keep-a-changelog": "^3.1.0",
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "release-it": "^15.5.1",
    "ton-compiler": "^2.3.0",
    "ton-core": ">=0.49.0",
    "ton-crypto": "^3.2.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "ton-core": ">=0.49.0",
    "ton-crypto": "^3.2.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/keep-a-changelog": {
        "filename": "CHANGELOG.md"
      }
    }
  },
  "dependencies": {
    "ohm-js": "^17.0.4"
  }
}
